{
  "title": "Test",
  "routes": [
    {
      "title": "List Organzations",
      "curl": "curl -s -H 'Content-type: application/json' -H 'Accept: application/json' http://localhost:8088/organization",
      "description": "Gets all organizations",
      "method": "GET",
      "url": "/organization",
      "params": [
        {
          "type": "uuid",
          "path": "id",
          "description": "id"
        },
        {
          "type": "enum",
          "path": "role",
          "values": [
            "admin",
            "manager",
            "agent"
          ],
          "example": "admin",
          "description": "The user's role"
        }
      ],
      "request": null,
      "requestNote": "Input Note",
      "responseNote": "Output Note",
      "exampleResponse": null,
      "originalUrl": "/organization"
    },
    {
      "title": "Create organization",
      "curl": "curl -s -H 'Content-type: application/json' -H 'Accept: application/json' http://localhost:8088/organization -X POST -d '{\"id\":\"885b440b-8b0e-445c-bd0f-b212ad0fdc41\"}'",
      "description": "Gets all organizations",
      "method": "POST",
      "url": "/organization",
      "request": [
        {
          "type": "uuid",
          "path": "id",
          "description": "id"
        }
      ],
      "requestNote": "Input Note",
      "responseNote": "Output Note",
      "exampleResponse": null,
      "originalUrl": "/organization"
    },
    {
      "title": "Create Department",
      "curl": "curl -s -H 'Content-type: application/json' -H 'Accept: application/json' http://localhost:8088/department -X POST",
      "description": "Creates a department",
      "method": "POST",
      "url": "/department",
      "responseNote": "Output Note",
      "exampleResponse": null,
      "originalUrl": "/department"
    },
    {
      "curl": "curl -s -H 'Content-type: application/json' -H 'Accept: application/json' http://localhost:8088/health_check",
      "description": "Return status of parent service",
      "exampleResponse": {
        "name": "helpdotcom-service"
      },
      "method": "GET",
      "originalUrl": "/health_check",
      "response": [
        {
          "name": "name",
          "type": "string",
          "path": "name",
          "example": "helpdotcom-service"
        }
      ],
      "title": "Health Check",
      "url": "/health_check"
    }
  ],
  "config": [
    {
      "name": "loglevel",
      "default": "info",
      "type": "STRING",
      "required": false,
      "env": "LOGLEVEL"
    },
    { "name": "port"
      , "default": 8088
      , "type": "NUMBER"
      , "required": false
      , "env": "PORT"
    },
    {
      "name": "timestamps",
      "default": false,
      "type": "BOOLEAN",
      "required": false,
      "env": "LOG_TIMESTAMPS",
      "isDefault": true
    },
    {
      "name": "name",
      "type": "STRING",
      "required": true,
      "env": "NAME",
      "isDefault": true
    },
    {
      "name": "db_pool_max",
      "type": "NUMBER",
      "required": false,
      "default": 50,
      "env": "DB_POOL_MAX",
      "isDefault": true
    },
    {
      "name": "db_pool_min",
      "type": "NUMBER",
      "required": false,
      "default": 2,
      "env": "DB_POOL_MIN",
      "isDefault": true
    },
    {
      "name": "db_pool_idle_timeout",
      "type": "NUMBER",
      "required": false,
      "default": 10000,
      "env": "DB_POOL_IDLE_TIMEOUT",
      "isDefault": true
    }
  ]
}
